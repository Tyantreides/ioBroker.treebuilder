    "@react-gifs/tools": "^0.1.2",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.18",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/sinon": "^10.0.12",
    "@types/sinon-chai": "^3.2.8",
    "@uiball/loaders": "^1.2.6",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
